{
    "name": "fabacks/random",
    "description": "A PHP class generate token, guid and string",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien COLAS",
            "email": "colas.fab@dahoo.fr"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Fabacks\\": "src/"
        }
    },
    "autoload-dev": {
